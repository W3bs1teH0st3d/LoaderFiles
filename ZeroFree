ope:FindFirstChild("Scope");if scopeInner then scopeInner.ImageTransparency=1;scopeInner.Size=UDim2.new(2,0,2,0);scopeInner.Position=UDim2.new( -0.5,0, -0.5,0);local blur=scopeInner:FindFirstChild("Blur");if blur then blur.ImageTransparency=1;local blurInner=blur:FindFirstChild("Blur");if blurInner then blurInner.ImageTransparency=1;end end end for i=1,4 do local frame=crosshairs:FindFirstChild("Frame"   .. i );if frame then frame.Transparency=1;end end else scope.ImageTransparency=0;local scopeInner=scope:FindFirstChild("Scope");if scopeInner then scopeInner.ImageTransparency=0;scopeInner.Size=UDim2.new(1,0,1,0);scopeInner.Position=UDim2.new(0,0,0,0);local blur=scopeInner:FindFirstChild("Blur");if blur then blur.ImageTransparency=0;local blurInner=blur:FindFirstChild("Blur");if blurInner then blurInner.ImageTransparency=0;end end end for i=1,4 do local frame=crosshairs:FindFirstChild("Frame"   .. i );if frame then frame.Transparency=0;end end end end end local blnd=gui:FindFirstChild("Blnd");if blnd then if _G.RemoveFlash then blnd.Enabled=false;else blnd.Enabled=true;end end end end);if (_G.RemoveBulletsHoles and Debris) then for _,v in pairs(Debris:GetChildren()) do if (v.Name=="Bullet") then v:Remove();end end end if (_G.RemoveSmoke and RayIgnore) then for _,v in pairs((RayIgnore:FindFirstChild("Smokes") and RayIgnore.Smokes:GetChildren()) or {} ) do if (v.Name=="Smoke") then v:Remove();end end end if (_G.RemoveBlood and Debris) then for _,v in pairs(Debris:GetChildren()) do if (v.Name=="SurfaceGui") then v:Remove();end end end local character=LocalPlayer.Character;if character then if _G.Noclip then for _,Instance in pairs(character:GetChildren()) do if Instance:IsA("BasePart") then Instance.CanCollide=false;end end else for _,Instance in pairs(character:GetChildren()) do if Instance:IsA("BasePart") then Instance.CanCollide=true;end end end end if (_G.Fly and LocalPlayer.Character) then local Speed=_G.FlySpeed or 16 ;local Velocity=Vector3.new(0,1,0);if UserInputService:IsKeyDown(Enum.KeyCode.W) then Velocity=Velocity + (Camera.CFrame.LookVector * Speed) ;end if UserInputService:IsKeyDown(Enum.KeyCode.A) then Velocity=Velocity + (Camera.CFrame.RightVector *  -Speed) ;end if UserInputService:IsKeyDown(Enum.KeyCode.S) then Velocity=Velocity + (Camera.CFrame.LookVector *  -Speed) ;end if UserInputService:IsKeyDown(Enum.KeyCode.D) then Velocity=Velocity + (Camera.CFrame.RightVector * Speed) ;end LocalPlayer.Character.HumanoidRootPart.Velocity=Velocity;LocalPlayer.Character.Humanoid.PlatformStand=true;end pcall(function() if (_G.Bhop and LocalPlayer.Character and UserInputService:IsKeyDown(Enum.KeyCode.Space) and (LocalPlayer.PlayerGui.GUI.Main.GlobalChat.Visible==false)) then LocalPlayer.Character.Humanoid.Jump=true;local Speed=_G.BhopSpeed or 100 ;local Dir=Camera.CFrame.LookVector * Vector3.new(1,0,1) ;local Move=Vector3.new();Move=(UserInputService:IsKeyDown(Enum.KeyCode.W) and (Move + Dir)) or Move ;Move=(UserInputService:IsKeyDown(Enum.KeyCode.S) and (Move-Dir)) or Move ;Move=(UserInputService:IsKeyDown(Enum.KeyCode.D) and (Move + Vector3.new( -Dir.Z,0,Dir.X))) or Move ;Move=(UserInputService:IsKeyDown(Enum.KeyCode.A) and (Move + Vector3.new(Dir.Z,0, -Dir.X))) or Move ;if (Move.Unit.X==Move.Unit.X) then Move=Move.Unit;LocalPlayer.Character.HumanoidRootPart.Velocity=Vector3.new(Move.X * Speed ,LocalPlayer.Character.HumanoidRootPart.Velocity.Y,Move.Z * Speed );end end end);end);library:SendNotification("ZeroSense | FREE",5,Color3.fromRGB(0,255,0)); end
